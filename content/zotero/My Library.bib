@article{cammiesExploringEconomicLegal2021,
  title = {Exploring Economic and Legal Barriers to Commercial Aquaponics in the {{EU}} through the Lens of the {{UK}} and Policy Proposals to Address Them},
  author = {Cammies, Christopher and Mytton, David and Crichton, Rosemary},
  date = {2021-06},
  journaltitle = {Aquaculture International},
  shortjournal = {Aquacult Int},
  volume = {29},
