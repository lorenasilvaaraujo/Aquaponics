@article{modarelliRecirculatingAquaponicsSystems2022,
  title = {Recirculating Aquaponics Systems: The New Era of Food Production},
  shorttitle = {Recirculating Aquaponics Systems},
  author = {Modarelli, G.C. and Cirillo, C. and Vanacore, L. and Langellotti, A.L. and Masi, P. and De Pascale, S. and Rouphael, Y.},
  date = {2022-09},
  journaltitle = {Acta Horticulturae},
