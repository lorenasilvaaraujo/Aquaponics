@article{babatundeResponseGerminationSeedling2022,
  title = {Response of Germination and Seedling Growth of {{Jute}} Plant ({{Corchorus}} Olitorius {{L}}.) on Three Different Substrates in the Tilapia Aquaponic System},
  author = {Babatunde, Taofik Ademola and Abdulkarim, Babangida and Wagini, Nasir Hassan and Usman, Sama'ila Adamu and Argungu, Lawali Alkali and Lawal, Umar},
  date = {2022-12},
  journaltitle = {Journal of Agriculture and Food Research},
  shortjournal = {Journal of Agriculture and Food Research},
  volume = {10},
