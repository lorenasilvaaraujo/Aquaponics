@article{quevedocascanteRoleExternalForces2022,
  title = {The Role of External Forces in the Adoption of Aquaculture Innovations: {{An}} Ex-Ante Case Study of Fish Farming in {{Colombia}}'s Southern {{Amazonian}} Region},
  shorttitle = {The Role of External Forces in the Adoption of Aquaculture Innovations},
  author = {Quevedo Cascante, MÃ³nica and Acosta GarcÃ­a, NicolÃ¡s and Fold, Niels},
  date = {2022-01},
  journaltitle = {Technological Forecasting and Social Change},
  shortjournal = {Technological Forecasting and Social Change},
  volume = {174},
  pages = {121185},
  issn = {00401625},
  doi = {10.1016/j.techfore.2021.121185},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0040162521006181},
  urldate = {2022-12-17},
  abstract = {Several adoption models have been developed to explain the dynamics behind the uptake of new technologies in food-production systems. However, the literature has yet to consider a range of external forces that affect farmersâ€™ decision-making processes. We argue that climate change and institutions are latent explanatory variÂ­ ables that require attention in the literature on aquatic-based innovations. Our aim is to conduct an ex-ante analysis focusing on these two external forces in the context of aquaculture and the adoption of aquaponics technology in Colombia. We use an embedded case-study design incorporating a qualitative and exploratory approach and employ two categories of fish-farming production systems as units of analysis. We triangulate our findings using non-probability sampling techniques and use our findings as a benchmark to discuss the potential adoption of aquaponics technology. Our findings suggest that fluctuations in rainfall and drought are the most important climate variables influencing negatively fish farming activities. Furthermore, we find that the complex institutional structures involved create unequal informal mechanisms among fish-farming production systems. We argue for context-specific designs when considering the adoption of aquaponics and conclude that, while fishfarming production systems encounter these external forces differently, heterogeneity also exists within systems, revealing intricacies worth considering.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\3PUJVTBZ\\Quevedo Cascante et al. - 2022 - The role of external forces in the adoption of aqu.pdf}
}

