@article{pinhoIntegratedProductionNile2021,
  title = {Integrated Production of {{Nile}} Tilapia Juveniles and Lettuce Using Biofloc Technology},
  author = {Pinho, Sara M. and David, Luiz Henrique C. and Goddek, Simon and Emerenciano, MaurÃ­cio G. C. and Portella, Maria CÃ©lia},
