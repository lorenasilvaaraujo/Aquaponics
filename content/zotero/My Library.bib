@article{zhuCarbonDynamicsEnergy2022,
  title = {Carbon Dynamics and Energy Recovery in a Novel Near-Zero Waste Aquaponics System with Onsite Anaerobic Treatment},
  author = {Zhu, Ze and Yogev, Uri and Goddek, Simon and Yang, Fei and Keesman, Karel J. and Gross, Amit},
  date = {2022-08},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
  volume = {833},
