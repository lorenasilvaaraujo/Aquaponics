@incollection{degraafSmarthoodsAquaponicsIntegrated2019,
  title = {Smarthoods: {{Aquaponics Integrated Microgrids}}},
  shorttitle = {Smarthoods},
  booktitle = {Aquaponics {{Food Production Systems}}},
  author = {de Graaf, Florijn and Goddek, Simon},
