@article{wenzelEffectsDissolvedPotassium2021,
  title = {Effects of {{Dissolved Potassium}} on {{Growth Performance}}, {{Body Composition}}, and {{Welfare}} of {{Juvenile African Catfish}} ({{Clarias}} Gariepinus)},
  author = {Wenzel, Lisa Carolina and Strauch, Sebastian Marcus and Eding, Ep and Presas-Basalo, Francisco Xose and Wasenitz, Berit and Palm, Harry Wilhelm},
