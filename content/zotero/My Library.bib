@article{yavuzcanyildizFishWelfareAquaponic2017,
  title = {Fish {{Welfare}} in {{Aquaponic Systems}}: {{Its Relation}} to {{Water Quality}} with an {{Emphasis}} on {{Feed}} and {{Faeces}}â€”{{A Review}}},
  shorttitle = {Fish {{Welfare}} in {{Aquaponic Systems}}},
  author = {Yavuzcan Yildiz, Hijran and Robaina, Lidia and Pirhonen, Juhani and Mente, Elena and DomÃ­nguez, David and Parisi, Giuliana},
  date = {2017-01-01},
  journaltitle = {Water},
  volume = {9},
