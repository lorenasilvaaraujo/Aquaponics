@article{nguyenDesignMonitoringControl2020,
  title = {Design of a monitoring and control system for Aquaponics based on Iot technology},
  author = {Nguyen, Nhan Chi and Nguyen, Khang Phuoc Hoang and Nguyen, Quan Hoang and Nguyen, Hieu Van and Ho, Huy Thanh},
  date = {2020-11-01},
  journaltitle = {Science and Technology Development Journal - Natural Sciences},
  shortjournal = {Sci. Tech. Dev. J. - Nat. Sci.},
  volume = {4},
