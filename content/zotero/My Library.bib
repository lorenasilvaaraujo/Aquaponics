@article{tahaRecentAdvancesSmart2022,
  title = {Recent {{Advances}} of {{Smart Systems}} and {{Internet}} of {{Things}} ({{IoT}}) for {{Aquaponics Automation}}: {{A Comprehensive Overview}}},
