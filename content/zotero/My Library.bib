  author = {Castilho-Barros, Leonardo and Almeida, FÃ¡bio H. and Henriques, Marcelo B. and Seiffert, Walter Q.},
  date = {2018-10},
  journaltitle = {Aquaculture International},
  shortjournal = {Aquacult Int},
  volume = {26},
