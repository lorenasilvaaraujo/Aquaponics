@article{wongkiewNitrogenRecoveryAquaponics2021,
  title = {Nitrogen {{Recovery}} via {{Aquaponics}}â€“{{Bioponics}}: {{Engineering Considerations}} and {{Perspectives}}},
  shorttitle = {Nitrogen {{Recovery}} via {{Aquaponics}}â€“{{Bioponics}}},
  author = {Wongkiew, Sumeth and Hu, Zhen and Lee, Jae Woo and Chandran, Kartik and Nhan, Hua Thai and Marcelino, Kyle Rafael and Khanal, Samir Kumar},
  date = {2021-03-12},
  journaltitle = {ACS ES\&T Engineering},
  shortjournal = {ACS EST Eng.},
  volume = {1},
  number = {3},
  pages = {326--339},
  issn = {2690-0645},
  doi = {10.1021/acsestengg.0c00196},
  url = {https://pubs.acs.org/doi/10.1021/acsestengg.0c00196},
  urldate = {2021-06-24},
  abstract = {Nitrogen, a critically important nutrient that boosts yields in agriculture and food production, is currently overused to meet the rising demand for food. Surplus nitrogen ends up in the environment in excess of the capacity of natural nitrogen cycle, thereby leading to serious environmental pollutions, such as eutrophication of water bodies and emission of nitrous oxide (a highly potent greenhouse gas) to atmosphere. Aquaponicsâˆ’ bioponics is an emerging soilless technology for nitrogen recovery that links organic vegetable production to aquaculture effluent remediation (aquaponics) or organic waste recycling (bioponics). This Review presents the concept of aquaponicsâˆ’bioponics for nitrogen recovery. Nitrogen transformations and nitrogen mass distributions in aquaponicâˆ’bioponic systems are critically discussed, along with the nutrient availability of several organic composts that can be integrated with the systems, and the microbial communities involved. This discussion is followed by a dynamic nitrogen modeling for managing nitrogen from different wastes in aquaponicsâˆ’bioponics. Various emerging engineering technologies that could improve aquaponicsâˆ’bioponics are presented, including aeration with microbubbles and/or nanobubbles, cocultivation with algae, process automation with Internet of Things, and integration with indoor vertical farming (plant factory with artificial light). Overall, the Review lays out the state-of-art in aquaponicsâˆ’bioponics and highlights potential approaches for developing highly efficient nitrogen recovery technologies from diverse organic waste streams.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\FK9I7JX2\\Wongkiew et al. - 2021 - Nitrogen Recovery via Aquaponicsâ€“Bioponics Engine.pdf}
}

@article{wongkiewNitrogenRecoveryNitrous2018,
  title = {Nitrogen Recovery and Nitrous Oxide ({{N2O}}) Emissions from Aquaponic Systems: {{Influence}} of Plant Species and Dissolved Oxygen},
  shorttitle = {Nitrogen Recovery and Nitrous Oxide ({{N2O}}) Emissions from Aquaponic Systems},
  author = {Wongkiew, Sumeth and Popp, Brian N. and Khanal, Samir Kumar},
