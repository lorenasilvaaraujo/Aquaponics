@article{bosireEducationSustainabilityFood2017,
  title = {Education for Sustainability and Food Literacy - Assessing Opportunities and Challenges for Using Aquaponics among Young People at {{Schools}}},
  author = {Bosire, Collins Momanyi and Breidahl, Katrine S and Sikora, Tomasz Adam and Mikkelsen, Bent Egberg},
  date = {2017},
  pages = {17},
  abstract = {Sustainable food production and sustainable diets along the food chain continue to be key challenges in the 21st century mainly due to population growth and urbanization. These also offer opportunities for new sustainable food production technologies. Aquaponics - an integrated closed-loop multi-trophic food production system that combines elements of a recirculating aquaculture system (RAS) and hydroponics is one of the approaches. But knowledge and understanding of these kind of new technologies is required at an early age. In other words, aquaponics could be turned into education for sustainability. There is potential for applying aquaponics on a wide spectrum of academic disciplines with a possibility for practical incorporation into existing educational curriculum. Literature about the use of aquaponics in education is currently inadequate and therefore this paper sought to identify possibilities and obstacles on how aquaponics can be affectively utilized in teaching primary school children. Data from three (N=3) independent qualitative interviews from: a biology teacher, a consultant entrepreneur and aquaponics expert, and one local aquaponics bio-farmer were obtained. The data analysis procedure was inspired by Future Workshop approach, leading to categorization and evaluation of statements from participants in three categories: Critique, Fantasy and Strategy. Some of the positive issues from informantsâ€™ perspectives included: a wide range of application in the subjects: biology, mathematics, science and more. Reduced pollution and efficient resource usage, flexibility of the system set up such as on rooftops, and production of organic-like twofold produce - fish and plant foods. Potential limitations included: time constraints, lack of financial resources as well as the need for frequent care and maintenance. The findings showed that visions of a new way of teaching with inclusion of the modern technology could be perceived as an advantage in influencing transformational processes at Danish schools. Nevertheless, this process requires some criticalconsiderations both practical and theoretical for implementation of the system in order to make it successful and sustainable in the long-term.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\LNIYHSR4\\Bosire et al. - 2017 - Education for sustainability and food literacy - a.pdf}
}

@article{bosmaFinancialFeasibilityProducing2017,
  title = {The Financial Feasibility of Producing Fish and Vegetables through Aquaponics},
  author = {Bosma, Roel H. and Lacambra, Lysette and Landstra, Ynze and Perini, Chiara and Poulie, Joline and Schwaner, Marie J. and Yin, Yi},
  date = {2017-08},
  journaltitle = {Aquacultural Engineering},
  volume = {78},
  pages = {146--154},
  issn = {01448609},
  doi = {10.1016/j.aquaeng.2017.07.002},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0144860917300821},
  urldate = {2019-07-03},
  abstract = {Aquaponics, producing fish and vegetables in a closed-loop water system, reduces fertilizer use and water discharge, and is therefore promoted as a sustainable venture. A recent global study found that the majority of 257 surveyed aquaponics farms made losses, but the reasons have been poorly analyzed. Based upon grey literature and a post-hoc cost-benefit analysis for an investment in an aquaponics farm in the Philippines, this paper aims to assess the factors contributing to an appropriate level of returns. In the Philippines, vegetables are relatively well-priced, but if catfish were produced, a venture producing 1250 kg fish, 6000 kg lettuce, and 300 kg tomato per year would have a Net-Benefit-Cost Ratio of below 1.3 after 20 years. This ratio should be higher when one considers the risks against which a farmer cannot and should not insure its production, as argued in the paper. Depending on the species of both fish and vegetables, the quantity of nutrients coming from the former component imposes a fish volume: vegetable area ratio ranging from 1:30 to 1:100, thus the quantity of marketable fresh vegetables determines the size of an aquaponics enterprise. As a consequence, the investments in the fish component are relatively high and weigh heavily on the financial balance. For producers to successfully adopt aquaponics, they need to consider starting first with catfish, and then, as they get to master the systemâ€™s management, they can shift to a high-value fish species for niche markets.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\YYWCL3R5\\Bosma et al. - 2017 - The financial feasibility of producing fish and ve.pdf}
}

