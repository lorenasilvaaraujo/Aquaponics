@article{filepPilotAquaponicGrowing2016,
  title = {Pilot {{Aquaponic Growing System}} of {{Carp}} ({{Cyprinus Carpio}}) and {{Basil}} ({{Ocimum Basilicum}})},
  author = {Filep, Radu Mihai and Diaconescu, Åžtefan and Costache, Mioara and Stavrescu-Bedivan, Mala-Maria and BÄƒdulescu, Liliana and Nicolae, Carmen Georgeta},
