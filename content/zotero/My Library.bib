@article{palmProportionalScalingAfrican2018,
  title = {Proportional up Scaling of {{African}} Catfish ( {{Clarias}} Gariepinus {{Burchell}}, 1822) Commercial Recirculating Aquaculture Systems Disproportionally Affects Nutrient Dynamics},
  author = {Palm, H.W. and Knaus, U. and Wasenitz, B. and Bischoff, A.A. and Strauch, S.M.},
  date = {2018-04},
  journaltitle = {Aquaculture},
  volume = {491},
