@article{campos-figueroaPresenceDipteraNuisance2015,
  title = {Presence of the {{Diptera Nuisance Non-biting Mosquito}}, {{Chironomus}} Sp., and {{Fungus Gnat}}, {{Bradysia}} Sp., on {{Lettuce}}, {{Lactuca}} Sativa, within an {{Aquaponic System}}},
  author = {Campos-Figueroa, Manuel and McArdle, Andrew},
  date = {2015-06},
  journaltitle = {Southwestern Entomologist},
  volume = {40},
  number = {2},
  pages = {439--440},
  issn = {0147-1724, 2162-2647},
  doi = {10.3958/059.040.0219},
  url = {http://www.bioone.org/doi/10.3958/059.040.0219},
  urldate = {2019-07-03},
  abstract = {The larva of the non-biting mosquito, Chironomus sp., feeds on roots of horticultural plants and can damage indoor horticultural crops, particularly lettuce, Lactuca sativa L., and young tomatoes, Solanum lycopersicum L. (Cranston 1987). Small swarms occur near deposits of water from filtration systems. The fungus gnat, Bradysia sp., is a nuisance of several greenhouse-grown plants (Howard et al. 1994). Harris et al. (1996) mentioned that larvae feed on roots and indirectly transmit disease (Gillespie and Menzies 1993). Fungus gnat adults are a nuisance to greenhouse operators, interiorscapers, and consumers and can be monitored with yellow sticky cards (Drees http://hortipm.tamu.edu/publications/Fgnats.html). We tested three colors of sticky traps to monitor Chironomus sp. and Bradysia sp. within an aquaponic system. Aquaponic systems use large tanks to cultivate fish where the water is filtered and allowed to gravity feed to the hydroponic component (raceways or growbeds), carrying constant nutrients to the plants that are floating on polystyrene rafts. The water is pumped back to the fish, restarting the cycle. In aquaponic-production systems, the benefits of soilless hydroponic plant production are retained in addition to production of fish. It is within the filtration and cracks between the plant rafts where small swarms of these insects congregate. Two identical aquaponic systems were used within a glass greenhouse at the Texas A\&M AgriLife Research and Extension Center at Weslaco, TX. Each re-circulating aquaponic system was 5,680-liters. They were composed of two 1,135-liter fish tanks, a series of filtration tanks, and two 1.2-m wide x 7.3-m long aerated hydroponic raceways. The fish tanks retained water for 60 minutes, with the hydroponic raceways retaining water for approximately 48 minutes. Chironomus sp. and Bradysia sp. adults were detected by randomly placing three cards, green, blue, and yellow sticky cards (15 x 10 cm; AlphaScents, Portland, OR) on each raceway (four replications total) and over filtration tanks (two replications total). Sticky traps were monitored for 2 weeks on six growth stages of one cultivar of romaine (Lactuca sativa, 'Dragoon'). Statistical analysis (PROCMIXED with repeated measurements, means separated by LSMeans, and data square-root transformed, SAS Institute 2009) showed numbers of Chironomus sp. and Bradysia sp. adults not significantly different among the traps on the raceways and filtration systems (Table 1). The non-significant results were caused by much variation. Three to four times more Bradysia sp. adults were caught on blue than other colors of traps on the raceways in both weeks. Chironomus sp. was caught twice as often on blue traps during the first week and slightly more than on other colors during the second week. Bradysia sp. was caught in the filtration system approximately twice as often on blue traps Downloaded From: https://bioone.org/journals/Southwestern-Entomologist on 2/2/2019 Terms of Use: https://bioone.org/terms-of-use Access provided by Fundacao Getulio Vargas SP},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\CB2SCQPU\\Campos-Figueroa and McArdle - 2015 - Presence of the Diptera Nuisance Non-Biting Mosqui.pdf}
}

