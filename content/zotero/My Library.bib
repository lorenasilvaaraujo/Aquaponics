@article{saezPollinationEfficiencyArtificial2019,
  title = {Pollination Efficiency of Artificial and Bee Pollination Practices in Kiwifruit},
  author = {SÃ¡ez, AgustÃ­n and Negri, Pedro and Viel, Matias and Aizen, Marcelo A.},
