@article{spechtSociallyAcceptableUrban2016,
  title = {Socially Acceptable Urban Agriculture Businesses},
  author = {Specht, Kathrin and Weith, Thomas and Swoboda, Kristin and Siebert, Rosemarie},
  date = {2016-03},
  journaltitle = {Agronomy for Sustainable Development},
  volume = {36},
  number = {1},
