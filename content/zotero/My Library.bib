@article{oliverComparisonFourArtificial2018,
  title = {Comparison of {{Four Artificial Light Technologies}} for {{Indoor Aquaponic Production}} of {{Swiss Chard}}, {{Beta}} Vulgaris, and {{Kale}}, {{Brassica}} Oleracea},
  shorttitle = {Comparison of {{Four Artificial Light Technologies}} for {{Indoor Aquaponic Production}} of {{Swiss Chard}}, {{{\emph{Beta}}}}{\emph{ Vulgaris}} , and {{Kale}}, {{{\emph{Brassica}}}}{\emph{ Oleracea}}},
  author = {Oliver, Luke P. and Coyle, Shawn D. and Bright, Leigh A. and Shultz, R. Charles and Hager, Janelle V. and Tidwell, James H.},
  date = {2018-10},
  journaltitle = {Journal of the World Aquaculture Society},
  volume = {49},
