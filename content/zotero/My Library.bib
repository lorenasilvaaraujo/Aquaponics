@article{tahaUsingDeepConvolutional2022,
  title = {Using {{Deep Convolutional Neural Network}} for {{Image-Based Diagnosis}} of {{Nutrient Deficiencies}} in {{Plants Grown}} in {{Aquaponics}}},
