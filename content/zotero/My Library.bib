  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\9PI6ZV5V\\de Graaf and Goddek - 2019 - Smarthoods Aquaponics Integrated Microgrids.pdf}
}

@incollection{delaideAerobicAnaerobicTreatments2019,
