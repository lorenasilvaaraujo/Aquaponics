@article{lugertReviewFishGrowth2016,
  title = {A Review on Fish Growth Calculation: Multiple Functions in Fish Production and Their Specific Application},
  shorttitle = {A Review on Fish Growth Calculation},
  author = {Lugert, Vincent and Thaller, Georg and Tetens, Jens and Schulz, Carsten and Krieter, Joachim},
  date = {2016-03},
  journaltitle = {Reviews in Aquaculture},
  volume = {8},
