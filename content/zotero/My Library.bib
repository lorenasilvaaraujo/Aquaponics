@article{pinhoSouthAmericanFish2021,
  title = {South {{American}} Fish Species Suitable for Aquaponics: A Review},
  shorttitle = {South {{American}} Fish Species Suitable for Aquaponics},
  author = {Pinho, Sara M. and David, Luiz H. and Garcia, Fabiana and Keesman, Karel J. and Portella, Maria CÃ©lia and Goddek, Simon},
  date = {2021-03-11},
  journaltitle = {Aquaculture International},
  shortjournal = {Aquacult Int},
