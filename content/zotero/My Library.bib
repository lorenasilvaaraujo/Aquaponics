@incollection{mikkelsenFoodSustainabilityScience2019,
  title = {Food, {{Sustainability}}, and {{Science Literacy}} in {{One Package}}? {{Opportunities}} and {{Challenges}} in {{Using Aquaponics Among Young People}} at {{School}}, a {{Danish Perspective}}},
  shorttitle = {Food, {{Sustainability}}, and {{Science Literacy}} in {{One Package}}?},
