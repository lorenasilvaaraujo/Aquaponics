  author = {Chaneva, Ganka and Tomov, Alexander and Paunov, Momchil and Hristova, Viktoria and Ganeva, Valentina and Mihaylova, Nikolina and Anev, Svetoslav and Krumov, Nikolay and Yordanova, Zhenya and Tsenov, Boris and Vassileva, Valya and Bonchev, Georgi and Zhiponova, Miroslava},
