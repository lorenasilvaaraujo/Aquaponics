  author = {Pinho, Sara Mello and Molinari, Diego and de Mello, Giovanni Lemos and Fitzsimmons, Kevin M. and Coelho Emerenciano, MaurÃ­cio Gustavo},
  options = {useprefix=true},
  date = {2017-06},
  journaltitle = {Ecological Engineering},
  volume = {103},
