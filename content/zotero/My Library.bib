@article{wongkiewBioponicSystemNitrogen2021,
  title = {Bioponic System for Nitrogen and Phosphorus Recovery from Chicken Manure: {{Evaluation}} of Manure Loading and Microbial Communities},
  shorttitle = {Bioponic System for Nitrogen and Phosphorus Recovery from Chicken Manure},
  author = {Wongkiew, Sumeth and Koottatep, Thammarat and Polprasert, Chongrak and Prombutara, Pinidphon and Jinsart, Wanida and Khanal, Samir Kumar},
  date = {2021-04},
  journaltitle = {Waste Management},
  shortjournal = {Waste Management},
  volume = {125},
