@article{puccinelliGrowthMineralRelations2022,
  title = {Growth and {{Mineral Relations}} of {{Beta}} Vulgaris Var. Cicla and {{Beta}} Vulgaris Ssp. Maritima {{Cultivated Hydroponically}} with {{Diluted Seawater}} and {{Low Nitrogen Level}} in the {{Nutrient Solution}}},
  author = {Puccinelli, Martina and Carmassi, Giulia and Botrini, Luca and Bindi, Antonio and Rossi, Lorenzo and Fierro-SaÃ±udo, Juan Francisco and Pardossi, Alberto and Incrocci, Luca},
