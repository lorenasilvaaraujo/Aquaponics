@article{andreolaSpentCoffeeGrounds2019,
  title = {Spent {{Coffee Grounds}} in the {{Production}} of {{Lightweight Clay Ceramic Aggregates}} in {{View}} of {{Urban}} and {{Agricultural Sustainable Development}}},
  author = {Andreola, Fernanda and Borghi, Alessandro and Pedrazzi, Simone and Allesina, Giulio and Tartarini, Paolo and Lancellotti, Isabella and Barbieri, Luisa},
  date = {2019-10-31},
  journaltitle = {Materials},
