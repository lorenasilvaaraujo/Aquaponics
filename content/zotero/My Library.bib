@article{nozziEvaluationDicentrarchusLabrax2016,
  title = {Evaluation of {{Dicentrarchus}} Labrax {{Meats}} and the {{Vegetable Quality}} of {{Beta}} Vulgaris Var. Cicla {{Farmed}} in {{Freshwater}} and {{Saltwater Aquaponic Systems}}},
