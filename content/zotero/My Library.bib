@article{ghandarDecisionSupportSystem2021,
  title = {A {{Decision Support System}} for {{Urban Agriculture Using Digital Twin}}: {{A Case Study With Aquaponics}}},
  shorttitle = {A {{Decision Support System}} for {{Urban Agriculture Using Digital Twin}}},
