  booktitle = {2020 {{IEEE}} 8th {{R10 Humanitarian Technology Conference}} ({{R10-HTC}})},
  author = {Concepcion II, Ronnie and Lauguico, Sandy and Alejandrino, Jonnel and De Guia, Justin and Dadios, Elmer and Bandala, Argel},
  date = {2020-12-01},
