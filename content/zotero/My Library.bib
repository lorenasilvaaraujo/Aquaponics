  shorttitle = {{{SEARCHING FOR SOLUTIONS IN AQUACULTURE}}},
  author = {RadosavljeviÄ‡, Vladimir and Ä†irkoviÄ‡, Miroslav and LjubojeviÄ‡, Dragana and JakiÄ‡ DimiÄ‡, Dobrila and MarkoviÄ‡, Zoran and Å½utiÄ‡, Jadranka and MiliÄ‡eviÄ‡, Vesna},
