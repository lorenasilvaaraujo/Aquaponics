@article{mcartneyEffectsProhexadioneCaGA42006,
  title = {Effects of {{Prohexadione-Ca}} and {{GA4}}+7 on {{Scarf Skin}} and {{Fruit Maturity}} in {{Apple}}},
  author = {McArtney, Steven and Ferree, Dave and Schmid, John and Obermiller, J.D. and Green, A.},
  date = {2006-12},
  journaltitle = {HortScience},
  volume = {41},
