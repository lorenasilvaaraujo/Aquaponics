}

@article{loveEnergyWaterUse2015,
  title = {Energy and Water Use of a Small-Scale Raft Aquaponics System in {{Baltimore}}, {{Maryland}}, {{United States}}},
  author = {Love, David C. and Uhl, Michael S. and Genello, Laura},
  date = {2015-09},
  journaltitle = {Aquacultural Engineering},
  volume = {68},
