@article{aniEffectStockingDensity2021,
  title = {Effect of Stocking Density on Growth Performance of Monosex {{Nile Tilapia}} ({{Oreochromis}} Niloticus) in the Aquaponic System Integrated with Lettuce ({{Lactuca}} Sativa)},
