@article{liltvedCanAquaponicSystems2012,
  title = {Can Aquaponic Systems Be Adapted to {{Norwegian}} Aquaculture ?},
  author = {Liltved, Helge and {Helge Liltved1*, Morten Homme2, Siv Lene Gangnes Skar3,} and {Ole-Kristian Hess-Erga1, Stein UIeberg4 and AsbjÃ¸rn Drengstig}},
  date = {2012},
  abstract = {The growing aquaculture industry raises concern about  environmental impacts. Recirculating aquaculture sysÂ­ tems (RAS) have been developed to respond to increasÂ­ ing environmental regulations in countries with limiÂ­ ted access to land and water. By combining RAS with  hydroponic systems, where plants for human consumpÂ­ tion grow in the nutrient rich recirculating water, seveÂ­ ral advantages related to energy conservation, water and  nutrient reuse can be obtained. Aquaponic systems vary  in size from small units to large commercial units. The  systems may hold fresh water fish species with producÂ­ tion of herbs, vegetables or fruits, or salt water species  with focus on algae production. So far, most experience  has been gained by using robust warm water fish species,  such as tilapia. By introducing cold water species like  trout or salmon in aquaponics, several challenges and  new requirements have to be resolved. In a project partly  funded by the Regional Research Fund of Agder and  Innovation Norway, a pilot scale aquaponic system has  been established at Evje in Aust-Agder county, Norway.  The goal of this project is to provide an economic and  practical analysis for cold water aquaponics, providing  technological solutions and business models.  Introduction Over the last two decades, the production of fry and smolt  at Norwegian hatcheries has been more than doubled,  and this trend is also expected through the next decade  (Figure 1). In order to increase the production capacity,  Norwegian smolt farmers must address more intensive  types of water treatment technologies, such as recircuÂ­ lating aquaculture systems (RAS) including advanced  water treatment to maintain optimum rearing condition.  The environmental impact of the aquaculture indusÂ­ try is scrutinized and raises growing environmental conÂ­ cerns. It becomes increasingly clear that the long term  sustainability of many aquaculture operations in NorÂ­ way, as well as in other countries, is dependent on the  ability to minimize and mitigate negative environmental  impacts. It has been calculated that the production of one  ton of rainbow trout produce in total 50 kg N and 9 kg  P as waste (Bergheim \& Ã…sgÃ¥rd 1996, Azevedo et al.  2011).  RAS have been developed to respond to the increasÂ­ing environmental regulations in countries with limited  access to land and water, and provides distinct advantages  over pond and cage-based forms of aquaculture. RAS are  often promoted as a way to reduce the volume of waste  discharge to the environment, yet the disposal of accumulated waste is always a major concern. Due to efficient  treatment in the recirculating loop, and reduced effluent  water flow, pollution from RAS to the external environÂ­ ment is much easier to control. In 2009/2010, a total of  214 smolt farms were in operation in Norway. Only some  10\% out of these farms have been converted to RAS. The  expected number of new intensively run RAS farms are some 6-8 per year (Drengstig et al. 2011). Greenhouse production and other forms of horticulture in controlled or semi-controlled environments are important industries in Norwegian and European counÂ­tries. The use of hydroponic systems, where the plants are growing in recirculating water enriched with nutriÂ­ents, are common in greenhouses. Such systems create a controlled environment for growing plants. By comÂ­bining aquaculture and hydroponic systems, several adÂ­vantages related to energy conservation, water and nutriÂ­ent reuse can be obtained.},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\LUD2JQYW\\Can_aquaponic_systems_be_adapted_to_Norw.pdf}
}

