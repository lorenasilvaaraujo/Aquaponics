@article{khastiniMicrobialConsortiaEffects2019,
  title = {Microbial Consortia Effects on the Yields of Water Spinach in Milkfish Aquaponics System},
  author = {Khastini, R O and Idaryanto, F R and {Alimuddin} and Wahyuni, I and Sari, I J and Puspita, N and Wahyuni, A F},
