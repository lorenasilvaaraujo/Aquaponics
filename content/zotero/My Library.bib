  title = {â€œ{{WETWALL}}â€ - an Innovative Design Concept for the Treatment of Wastewater at an Urban Scale},
  author = {Castellar Da Cunha, Joana America and Arias, Carlos A. and Carvalho, Pedro and Rysulova, Martina and Canals, Joan Montserrat and Perez, Gabriel and Gonzalez, Montserrat Bosch and MoratÃ³, Jordi Farreras},
