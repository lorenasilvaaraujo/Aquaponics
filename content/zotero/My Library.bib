@article{fischerComparingWaterQuality2021,
  title = {Comparing Water Quality/Bacterial Composition and Productivity of Largemouth Bass {{Micropterus}} Salmoides Juveniles in a Recirculating Aquaculture System versus Aquaponics as Well as Plant Growth/Mineral Composition with or without Media},
  author = {Fischer, Hayden and Romano, Nicholas and Jones, Jacob and Howe, John and Renukdas, Nilima and Sinha, Amit Kumar},
  date = {2021-05},
  journaltitle = {Aquaculture},
