  editor = {{Agriculture Department, College of Agriculture and Natural Resources, Bohol Island State University, Zamora Bilar, Bohol}},
  date = {2017-12-15},
  journaltitle = {Science and Humanities Journal},
  shortjournal = {SHJ},
