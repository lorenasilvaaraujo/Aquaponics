@article{wongkiewAquaponicSystemsSustainable2018,
  title = {Aquaponic {{Systems}} for {{Sustainable Resource Recovery}}: {{Linking Nitrogen Transformations}} to {{Microbial Communities}}},
  shorttitle = {Aquaponic {{Systems}} for {{Sustainable Resource Recovery}}},
  author = {Wongkiew, Sumeth and Park, Mee-Rye and Chandran, Kartik and Khanal, Samir Kumar},
  date = {2018-11-06},
  journaltitle = {Environmental Science \& Technology},
  shortjournal = {Environ. Sci. Technol.},
  volume = {52},
