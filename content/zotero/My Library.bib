  title = {Estimation of {{Photosynthetic Growth Signature}} at the {{Canopy Scale Using New Genetic Algorithm-Modified Visible Band Triangular Greenness Index}}},
  booktitle = {2020 {{International Conference}} on {{Advanced Robotics}} and {{Intelligent Systems}} ({{ARIS}})},
  author = {Concepcion, Ronnie S. and Lauguico, Sandy C. and Tobias, Rogelio Ruzcko and Dadios, Elmer P. and Bandala, Argel A. and Sybingco, Edwin},
  date = {2020-08},
  pages = {1--6},
  publisher = {{IEEE}},
