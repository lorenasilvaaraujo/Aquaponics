@article{liantoContinuousInnovationLiterature2018,
  title = {Continuous {{Innovation}}: {{A Literature Review}} and {{Future Perspective}}},
  shorttitle = {Continuous {{Innovation}}},
  author = {Lianto, Benny and Dachyar, M. and Soemardi, Tresna Priyana},
  date = {2018-06-10},
  journaltitle = {International Journal on Advanced Science, Engineering and Information Technology},
  volume = {8},
  number = {3},
