@article{ebelingEngineeringAnalysisStoichiometry2006,
  title = {Engineering Analysis of the Stoichiometry of Photoautotrophic, Autotrophic, and Heterotrophic Removal of Ammoniaâ€“Nitrogen in Aquaculture Systems},
