  shorttitle = {Sustainable Development of Climate-Resilient Aquaculture and Culture-Based Fisheries through Adaptation of Abiotic Stresses},
  author = {Abisha, R. and Krishnani, Kishore Kumar and Sukhdhane, Kapil and Verma, A. K. and Brahmane, Manoj and Chadha, N. K.},
  date = {2022-07-01},
  journaltitle = {Journal of Water and Climate Change},
