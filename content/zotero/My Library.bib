@article{strauchCommercialAfricanCatfish2018,
  title = {Commercial {{African Catfish}} ({{Clarias}} Gariepinus) {{Recirculating Aquaculture Systems}}: {{Assessment}} of {{Element}} and {{Energy Pathways}} with {{Special Focus}} on the {{Phosphorus Cycle}}},
  shorttitle = {Commercial {{African Catfish}} ({{Clarias}} Gariepinus) {{Recirculating Aquaculture Systems}}},
  author = {Strauch, Sebastian and Wenzel, Lisa and Bischoff, Adrian and Dellwig, Olaf and Klein, Jan and SchÃ¼ch, Andrea and Wasenitz, Berit and Palm, Harry},
  date = {2018-05-30},
  journaltitle = {Sustainability},
  volume = {10},
  number = {6},
  pages = {1805},
  issn = {2071-1050},
  doi = {10.3390/su10061805},
  url = {http://www.mdpi.com/2071-1050/10/6/1805},
  urldate = {2019-01-18},
  abstract = {The reuse of effluent waters and sediments from African catfish (Clarias gariepinus) recirculation aquaculture systems requires a deeper understanding of the nutrient and energy flows and material pathways. Three semi-commercial systems, differing in stocking density, were sampled for nutritive and pollutant elements of the input- (tap water, feed) and output pathways (fillet, carcass, process water, sediments) by ICP-OES/MS and calorimetry. Highly water-soluble elements, e.g., potassium, accumulated in the water, whereas iron, copper, chromium and uranium where found in the solids. Feed derived phosphorous was accounted for, 58.3â€“64.2\% inside the fish, 9.7â€“19.3\% in sediments, and small amounts 9.6â€“15.5\% in the process waters. A total of 7.1â€“9.9\% of the feed accumulated as dry matter in the sediments, comprising 5.5â€“8.7\% total organic carbon and 3.7â€“5.2\% nitrogen. A total of 44.5â€“47.1\% of the feed energy was found in the fish and 5.7â€“7.7\% in the sediments. For reuse of water and nutrients in hydroponics, the macro-nutrients potassium, nitrate, phosphorus and the micro-nutrient iron were deficient when compared with generalized recommendations for plant nutrition. Low energy contents and C/N-ratio restrict the solely use of African catfish solids for biogas production or vermiculture. Using the outputs both for biogas supplement and general fertilizer in aquaponics farming (s.l.) (combined with additional nutrients) appears possible.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\EHXG5RYM\\Strauch et al. - 2018 - Commercial African Catfish (Clarias gariepinus) Re.pdf}
}

@article{strauchEffectsOrthoPhosphateGrowth2019,
  title = {Effects of {{Ortho-Phosphate}} on {{Growth Performance}}, {{Welfare}} and {{Product Quality}} of {{Juvenile African Catfish}} ({{Clarias}} Gariepinus)},
  author = {Strauch, Sebastian and Bahr, Judith and BaÃŸmann, BjÃ¶rn and Bischoff, Adrian and Oster, Michael and Wasenitz, Berit and Palm, Harry},
  date = {2019-01-23},
  journaltitle = {Fishes},
  volume = {4},
  number = {1},
  pages = {3},
  issn = {2410-3888},
  doi = {10.3390/fishes4010003},
  url = {http://www.mdpi.com/2410-3888/4/1/3},
  urldate = {2019-06-29},
  abstract = {Orthoâ€phosphate inside recirculation aquaculture systems is limited as a consequence of precipitation and regular water exchange rates. To improve plant growth in coupled aquaponics, phosphate fertilizer addition to hydroponics can increase PO43âˆ’â€P concentrations inside the process water. We investigated the effects of four PO43âˆ’â€P concentrations ({$<$}10 (P0), 40, 80, 120 mg Lâˆ’1) in rearing water on growth performance, feed efficiency, and welfare traits of juvenile African catfish (Clarias gariepinus Burchell, 1822). By trend, optimum specific growth rate of 2.66\% dâˆ’1 and feed conversion ratio of 0.71 were observed at 40 and 80 mg Lâˆ’1 PO43âˆ’â€P. Higher PO43âˆ’â€P significantly affected skin coloration, swimming activity and external injuries, with the palest and inactive fish combined with most external injuries in the P120 group. Mineral and protein contents in the fish remained unaffected, while fat content inside the fillets enriched with increasing PO43âˆ’â€P. Inorganic P in blood plasma increased significantly, while phosphate concentrations inside the fillet remained unchanged. We suggest that PO43âˆ’â€P concentrations of 40 to 80 mg Lâˆ’1 do not reduce the performance of African catfish aquaculture, while increased values of 120 mg Lâˆ’1 affect fish welfare. This allows limited addition of PO43âˆ’â€P fertilizer in coupled aquaponics with African catfish to support plant growth.},
  langid = {english},
  keywords = {â­•},
  file = {D\:\\Zotero\\storage\\44S9GY9B\\Strauch_et_al_2019_Effects_of_ortho-phosphate_on.pdf;D\:\\Zotero\\storage\\I7UYAMEL\\Strauch et al. - 2019 - Effects of Ortho-Phosphate on Growth Performance, .pdf}
}

