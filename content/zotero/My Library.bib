  author = {Schmautz, Zala and Graber, Andreas and Jaenicke, Sebastian and Goesmann, Alexander and Junge, Ranka and Smits, Theo H. M.},
  date = {2017-05},
  journaltitle = {Archives of Microbiology},
  volume = {199},
